{
  "name": "n8n-nodes-fillpdf",
  "version": "1.0.0",
  "description": "n8n community node for automated PDF form filling using the fillpdf Python library. Fill PDF forms with workflow data, handle multiple field types, and generate documents programmatically.",
  "keywords": [
    "n8n-community-node-package",
    "pdf",
    "forms",
    "fillpdf",
    "automation",
    "document-generation",
    "form-filling",
    "python",
    "workflow",
    "n8n"
  ],
  "license": "MIT",
  "homepage": "https://github.com/tanutcho/n8n-nodes-fillpdf#readme",
  "bugs": {
    "url": "https://github.com/tanutcho/n8n-nodes-fillpdf/issues"
  },
  "author": {
    "name": "Tanut Choksatchawathi",
    "email": "tanut.cho@gmail.com",
    "url": "https://github.com/tanutcho"
  },
  "contributors": [
    {
      "name": "Tanut Choksatchawathi",
      "email": "tanut.cho@gmail.com",
      "url": "https://github.com/tanutcho"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanutcho/n8n-nodes-fillpdf.git"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "build:clean": "rimraf dist && npm run build",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "format:check": "prettier nodes --check",
    "lint": "tsc --noEmit && eslint nodes --ext .ts,.js && npm run format:check",
    "lint:fix": "eslint nodes --fix --ext .ts,.js && npm run format",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "validate-setup": "node setup-validator.js",
    "validate-publication": "node validate-publication.js",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "prepack": "npm run build:clean && npm run lint && npm run validate-publication",
    "prepublishOnly": "npm run prepack",
    "release": "npm run prepack && npm publish",
    "release:dry": "npm run prepack && npm publish --dry-run"
  },
  "files": [
    "dist/**/*",
    "setup-validator.js",
    "validate-publication.js",
    "INSTALLATION.md",
    "INSTALLATION-CHECKLIST.md",
    "TROUBLESHOOTING.md",
    "EXAMPLES.md",
    "CONTRIBUTING.md",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/FillPdf/FillPdf.node.js"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.16.16",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^5.0.0",
    "jest": "^29.5.0",
    "n8n-workflow": "^1.2.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "overrides": {
    "form-data": "^4.0.4"
  }
}